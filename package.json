    "jest": "26.2.2",
    "jest-cli": "26.2.2",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.3",
    "puppeteer": "5.2.1",
    "tailwindcss": "^1.6.2",
    "@types/quill": "^2.0.3",
    "quill": "^1.3.6",
    "moment": "^2.27.0",
